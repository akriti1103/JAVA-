/*matrix in java*/
public class MatrixAdd{
public static void main(String[] args){
//two matrix needed to have addition result
int m[][]={{1,3,4},{2,4,3},{3,4,5}};
int n[][]={{1,3,4},{2,4,3},{1,2,4}};
//third matrix for the result
int p[][]= new int[3][3];
//addition operator
for(int i=0;i<3;i++){
for(int j=0;j<3;j++){
p[i][j]=m[i][j]+n[i][j];
System.out.println(p[i][j]+" ");
}
System.out.println();
}
}
}
